<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gsitm.mydata.api.insu.irps.list.IrpListMapper">

	<!-- 개인형 IRP 계좌 목록 조회 -->
    <select id="findAll" parameterType="IrpDto" resultType="IrpDto">
        SELECT 
			   "1" AS prodName
			 , "1" AS accountNum
			 , "1" AS seqno
			 , "1" AS isConsent
			 , "1" AS accountStatus
    </select>
    
</mapper>